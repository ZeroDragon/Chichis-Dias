{
  "name": "Chichis Dias",
  "version": "1.5",
  "description": "Extension para subir chichis al twitpic y mandar el twitt.",
  "options_page": "options.html",
  "background_page" : "background.html",
  "permissions": [
    "contextMenus",
    "tabs",
    "http://*/",
    "https://*/"
  ],
  "icons": {
     "128": "icon.png",
     "16": "icon.png",
     "48": "icon.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "popup": "popup.html"
  }
}
